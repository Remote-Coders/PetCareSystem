<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_blue"
    tools:context=".MainActivity10">

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#081C34"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <ImageView
        android:id="@+id/dogfood_1"
        android:layout_width="167dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dogfoods"
        app:srcCompat="@drawable/foodbg"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/dogfood_2"
        android:layout_width="158dp"
        android:layout_height="146dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toEndOf="@+id/dogfood_1"
        app:layout_constraintTop_toBottomOf="@+id/tv_dogfoods"
        app:srcCompat="@drawable/foodbg"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/dogfood_3"
        android:layout_width="163dp"
        android:layout_height="164dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="188dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dogfood_1"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/foodbg"
        android:contentDescription="@string/todo" />

    <TextView
        android:id="@+id/tv_category1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dog_food1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_1"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_1"
        app:layout_constraintStart_toStartOf="@+id/dogfood_1" />

    <TextView
        android:id="@+id/tv_category2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dog_food2"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_2"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_2"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="@+id/dogfood_2" />

    <TextView
        android:id="@+id/tv_category3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dog_food3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_3"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_3"
        app:layout_constraintStart_toStartOf="@+id/dogfood_3" />

    <TextView
        android:id="@+id/tv_dogfoods"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="@string/dog_food"
        android:textColor="#FFFCFC"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/im_back"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="34dp"
        android:layout_height="36dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/textbg"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/im_back"
        android:layout_width="34dp"
        android:layout_height="36dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="65dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/textbg"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="16dp"
        android:layout_height="24dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/im_back"
        app:layout_constraintEnd_toEndOf="@+id/im_back"
        app:layout_constraintStart_toStartOf="@+id/im_back"
        app:layout_constraintTop_toTopOf="@+id/im_back"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_24" />

    <ImageView
        android:id="@+id/im_favourite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/ic_baseline_favorite_24" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="98dp"
        android:layout_height="89dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_category3"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_3"
        app:layout_constraintStart_toStartOf="@+id/dogfood_3"
        app:layout_constraintTop_toTopOf="@+id/dogfood_3"
        app:srcCompat="@drawable/df2" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="73dp"
        android:layout_height="84dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_category2"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_2"
        app:layout_constraintStart_toStartOf="@+id/dogfood_2"
        app:layout_constraintTop_toTopOf="@+id/dogfood_2"
        app:srcCompat="@drawable/dog_food1_2" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="79dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_category1"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_1"
        app:layout_constraintStart_toStartOf="@+id/dogfood_1"
        app:layout_constraintTop_toTopOf="@+id/dogfood_1"
        app:srcCompat="@drawable/dog_food5_1" />
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="411dp"
        android:layout_height="49dp"
        android:background="@color/black"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintStart_toStartOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        app:srcCompat="@drawable/ic_baseline_menu_24" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="253dp"
        android:layout_height="31dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="2dp"
        android:background="@android:color/darker_gray"
        android:queryHint="@string/search"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintCircleRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintStart_toEndOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:targetApi="s">

    </SearchView>

</androidx.constraintlayout.widget.ConstraintLayout>