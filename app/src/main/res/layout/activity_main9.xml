<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_blue"
    tools:context=".MainActivity9">

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#081C34"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <ImageView
        android:id="@+id/dogfood_1"
        android:layout_width="160dp"
        android:layout_height="161dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/foodbg" />

    <ImageView
        android:id="@+id/dogfood_2"
        android:layout_width="162dp"
        android:layout_height="164dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dogfood_1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/foodbg" />

    <ImageView
        android:id="@+id/dogfood_3"
        android:layout_width="163dp"
        android:layout_height="164dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="188dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dogfood_1"
        app:srcCompat="@drawable/foodbg" />

    <ImageView
        android:id="@+id/logo2"
        android:layout_width="91dp"
        android:layout_height="99dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_3"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toStartOf="@+id/dogfood_3"
        app:layout_constraintTop_toTopOf="@+id/dogfood_3"
        app:srcCompat="@drawable/logo_200x200" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="91dp"
        android:layout_height="99dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_2"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="@+id/dogfood_2"
        app:layout_constraintTop_toTopOf="@+id/dogfood_2"
        app:srcCompat="@drawable/logo_200x200" />

    <ImageView
        android:id="@+id/logo1"
        android:layout_width="91dp"
        android:layout_height="99dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_1"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="@+id/dogfood_1"
        app:layout_constraintTop_toTopOf="@+id/dogfood_1"
        app:srcCompat="@drawable/logo_200x200" />

    <TextView
        android:id="@+id/tv_category1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/food_category1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_1"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_1"
        app:layout_constraintStart_toStartOf="@+id/dogfood_1"
        app:layout_constraintTop_toBottomOf="@+id/logo1" />

    <TextView
        android:id="@+id/tv_category2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/food_category2"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_2"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_2"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="@+id/dogfood_2"
        app:layout_constraintTop_toBottomOf="@+id/logo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_category3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/food_category3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dogfood_3"
        app:layout_constraintEnd_toEndOf="@+id/dogfood_3"
        app:layout_constraintStart_toStartOf="@+id/dogfood_3"
        app:layout_constraintTop_toBottomOf="@+id/logo2" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="411dp"
        android:layout_height="49dp"
        android:background="@color/black"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintStart_toStartOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        app:srcCompat="@drawable/ic_baseline_menu_24" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="253dp"
        android:layout_height="31dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="2dp"
        android:background="@android:color/darker_gray"
        android:queryHint="@string/search"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintCircleRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintStart_toEndOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:targetApi="s">

    </SearchView>

</androidx.constraintlayout.widget.ConstraintLayout>